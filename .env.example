# Stripe Configuration
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here
STRIPE_PRO_MONTHLY_PRICE_ID=price_your_pro_monthly_price_id
STRIPE_PRO_ANNUAL_PRICE_ID=price_your_pro_annual_price_id
STRIPE_MAX_MONTHLY_PRICE_ID=price_your_max_monthly_price_id
STRIPE_MAX_ANNUAL_PRICE_ID=price_your_max_annual_price_id

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
